<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Archivos</display-name>
  <servlet>
    <display-name>UploadFiles</display-name>
    <servlet-name>UploadFiles</servlet-name>
    <servlet-class>com.uploadFiles.UploadFiles</servlet-class>
  </servlet>
  <servlet>
    <display-name>DownloadFiles</display-name>
    <servlet-name>DownloadFiles</servlet-name>
    <servlet-class>com.downloadFiles.DownloadFiles</servlet-class>
  </servlet>
  <servlet>
    <display-name>DirFilesUpload</display-name>
    <servlet-name>DirFilesUpload</servlet-name>
    <servlet-class>com.DirFiles.DirFilesUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadFiles</servlet-name>
    <url-pattern>/UploadFiles</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadFiles</servlet-name>
    <url-pattern>/DownloadFiles</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DirFilesUpload</servlet-name>
    <url-pattern>/DirFilesUpload</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>GenerarZipDownload</display-name>
    <servlet-name>GenerarZipDownload</servlet-name>
    <servlet-class>com.GenerarZip.GenerarZipDownload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GenerarZipDownload</servlet-name>
    <url-pattern>/GenerarZipDownload</url-pattern>
  </servlet-mapping>
</web-app>